  "main": "dist/server/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
