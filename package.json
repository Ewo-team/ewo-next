    "@types/react-dom": "^16.8.4",
    "async": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "express-socket.io-session": "^1.3.5",
    "immutable": "^4.0.0-rc.12",
    "js-extract": "^1.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "redux": "^4.0.1",
    "redux-node-logger": "^0.1.0",
