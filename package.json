{
  "name": "ewo",
  "version": "1.0.0",
  "main": "dist/server/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --silent --outputFile=test/jest-test-results.json",
    "server": "nodemon -e ts -w ./src -x npm run server:watch",
    "server:watch": "node -r ts-node/register -r tsconfig-paths/register --inspect src/index.ts --lib node",
    "client": "webpack --watch",
    "prestorybook": "node-sass ./src/client/App.scss ./dist/client/App.css",
    "storybook": "start-storybook -p 6006",
    "docs": "node ./node_modules/typedoc/bin/typedoc src --logger none",
    "copy:coverage": "node ./test/docs-coverage"
  },
  "dependencies": {
    "async": "^3.1.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "express-socket.io-session": "^1.3.5",
    "express-validator": "^6.1.0",
    "immutable": "^4.0.0-rc.12",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "node-sass-middleware": "^0.11.0",
    "pug": "^2.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io": "^2.2.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/async": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/express-socket.io-session": "^1.3.2",
    "@types/jest": "^24.0.13",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
    "@types/node-sass-middleware": "^0.0.31",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/storybook__react": "^4.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "nodemon": "^1.19.1",
    "pathfinding": "^0.4.18",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-router": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-storybook-addon-props-combinations": "^1.1.0",
    "react-test-renderer": "^16.9.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "renderkid": "^2.0.3",
    "sass-loader": "^7.1.0",
    "session-file-store": "^1.3.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "0.11.1",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "jest": {
    "rootDir": "src",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^@server(.*)$": "<rootDir>/server$1",
      "^@client(.*)$": "<rootDir>/client$1",
      "^@models(.*)$": "<rootDir>/engine/models$1",
      "^@engine(.*)$": "<rootDir>/engine$1",
      "^@tasks(.*)$": "<rootDir>/engine/Commands/tasks$1",
      "^@commands(.*)$": "<rootDir>/engine/Commands/CommandsTemplate$1",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/**/*.{ts,tsx}",
      "!**/*.stories.{ts,tsx}"
    ],
    "coverageDirectory": "<rootDir>/../test",
    "coverageReporters": [
      "text",
      "json",
      "lcov",
      "text-summary",
      "clover",
      "cobertura"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "output": "<rootDir>../test/junit.xml"
        }
      ]
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.test\\.(ts|tsx|js)$"
  }
}