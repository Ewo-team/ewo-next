    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/node-sass-middleware": "^0.0.31",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/redux-thunk": "^2.1.0",
    "@types/socket.io": "^2.1.2",
    "async": "^3.0.1",
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "express-socket.io-session": "^1.3.5",
    "express-validator": "^5.3.1",
    "http-errors": "^1.7.2",
    "immutable": "^4.0.0-rc.12",
    "js-extract": "^1.0.1",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "node-sass-middleware": "^0.11.0",
    "pug": "^2.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io": "^2.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "css-loader": "^3.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "redux-mock-store": "^1.5.3",
    "redux-node-logger": "^0.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.3",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "tslint-config-airbnb": "^5.11.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.test\\.(ts|tsx|js)$"
  }
}
