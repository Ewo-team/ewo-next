{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-clean-code",
    "tslint-sonarts"
  ],
  "jsRules": {},
  "rulesDirectory": [
    "tslint-consistent-codestyle",
    "node_modules/tslint-microsoft-contrib"
  ],
  "rules": {
    /* TSLint Standards Rules */
    "max-classes-per-file": true,
    "max-file-line-count": true,
    "no-namespace": true,
    "no-console": false,
    /* Deactivation of some rules */
    "arrow-parens": false,
    "import-name": false,
    "interface-name": false,
    "max-line-length": [
      true,
      160
    ],
    "no-boolean-literal-compare": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "ter-arrow-parens": false,
    "variable-name": false,
    /* TSLint ESLint Rules*/
    "no-control-regex": true,
    "no-empty-character-class": true,
    "no-ex-assign": true,
    "no-extra-boolean-cast": true,
    "no-invalid-regexp": true,
    "no-regex-spaces": true,
    "no-sparse-arrays": true,
    /* TSLint Consistent Codestyle Rules*/
    "no-accessor-recursion": true,
    "no-collapsible-if": true,
    "no-return-undefined": true,
    "no-static-this": true,
    "no-unnecessary-else": true,
    "no-unnecessary-type-annotation": [
      true,
      "check-return-type"
    ],
    "no-unused": [
      true,
      "unused-function-expression-name",
      "unused-class-expression-name",
      "unused-catch-binding"
    ],
    "no-var-before-return": [
      true,
      "allow-destructuring"
    ],
    "prefer-const-enum": true,
    "prefer-while": true,
    /* TSLint React Rules*/
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-space-before-trailing-slash": true,
    /* TSLint Clean Code Rules */
    "max-func-args": [
      true,
      5
    ],
    "no-flag-args": true,
    "no-map-without-usage": true,
    "no-complex-conditionals": true,
    "prefer-dry-conditionals": true,
    /* TSLint Microsoft Contrib Rules */
    "insecure-random": true,
    "max-func-body-length": [
      true,
      100,
      {
        "ignore-parameters-to-function-regex": "^describe$"
      }
    ],
    "no-banned-terms": true,
    "no-cookies": true,
    "no-delete-expression": true,
    "no-disable-auto-sanitization": true,
    "no-document-domain": true,
    "no-document-write": true,
    "no-exec-script": true,
    "no-for-in": true,
    "no-function-expression": true,
    "no-inner-html": true,
    "no-octal-literal": true,
    "no-string-based-set-immediate": true,
    "no-string-based-set-interval": true,
    "no-string-based-set-timeout": true,
    "no-typeof-undefined": true,
    "no-unnecessary-field-initialization": true,
    "no-unnecessary-local-variable": true,
    "no-unnecessary-override": true,
    "no-useless-files": true,
    "no-with-statement": true,
    "non-literal-require": true,
    "possible-timing-attack": true,
    "prefer-array-literal": true,
    "promise-must-complete": true,
    /*"react-a11y-accessible-headings": true,*/
    "react-a11y-anchors": true,
    "react-a11y-aria-unsupported-elements": true,
    "react-a11y-event-has-role": true,
    "react-a11y-image-button-has-alt": true,
    "react-a11y-img-has-alt": true,
    /*"react-a11y-input-elements": true,*/
    "react-a11y-lang": true,
    "react-a11y-meta": true,
    /*"react-a11y-no-onchange": true,*/
    "react-a11y-props": true,
    "react-a11y-proptypes": true,
    /*"react-a11y-required": true,*/
    "react-a11y-role": true,
    "react-a11y-role-has-required-aria-props": true,
    "react-a11y-role-supports-aria-props": true,
    "react-a11y-tabindex-no-positive": true,
    "react-a11y-titles": true,
    "react-anchor-blank-noopener": true,
    "react-iframe-missing-sandbox": true,
    "react-no-dangerous-html": true,
    "react-unused-props-and-state": true,
    "use-named-parameter": true
    /*"use-simple-attributes": true*/
  }
}